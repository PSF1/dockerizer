#!/bin/bash

function dkHelp () {
  echo "Dockerizer command: symfony"
  echo
  echo "Symfony CLI, accepts all common software params."
}

while [ -n "$1" ]; do
    case "$1" in
        --dkhelp)
            dkHelp
            exit
            ;;
    esac
done

# source command_wrapper [CONTAINER NAME] [COMMAND] $@ = params to send.
source _command_wrapper_exec 'php' 'COLUMNS=160 symfony' $@
